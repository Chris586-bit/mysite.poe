{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/language": [
      "default",
      {
        "html-lang-valid": "off"
      }
    ],
    "highest-available-document-mode": "off",
    "meta-viewport": "off",
    "no-inline-styles": "off",
    "axe/name-role-value": "off",
    "axe/text-alternatives": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ]
  }
}