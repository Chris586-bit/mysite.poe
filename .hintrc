{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/language": [
      "default",
      {
        "html-lang-valid": "off"
      }
    ],
    "highest-available-document-mode": "off",
    "meta-viewport": "off",
    "no-inline-styles": "off",
    "axe/name-role-value": "off",
    "axe/text-alternatives": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "compat-api/css": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 137",
    "not firefox <= 137",
    "not ios_saf <= 18.4",
    "not safari <= 18.4"
  ]
}